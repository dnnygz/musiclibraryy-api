{
  "name": "musiclibrary-api",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend REST API for MusicLibrary system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:setup": "export $(cat .env | grep DATABASE_URL) && psql $DATABASE_URL -f scripts/setup-db.sql",
    "db:seed": "tsx scripts/seed-data.ts"
  },
  "keywords": [
    "music",
    "library",
    "api",
    "hono",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.0.0",
    "@hono/node-server": "^1.8.0",
    "postgres": "^3.4.0",
    "zod": "^3.22.0",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}

